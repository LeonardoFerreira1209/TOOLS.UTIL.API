      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SwaggerInfo": {
    "ApiDescription": "Api de envio de e-mails para o projeto. (TOOLS.API).",
    "ApiVersion": "tools.mail.api.v1",
    "UriMyGit": "https://github.com/LeonardoFerreira1209"
  },
  "RetryPolicy": {
    "RetryOn": "true",
    "RetryCount": 3,
